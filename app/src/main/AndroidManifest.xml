<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.circuitgen">

    <application
        android:allowBackup="true"
        android:icon="@drawable/sports"
        android:label="@string/app_name"
        android:roundIcon="@drawable/sports"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ScheduleOfTheDay"></activity>
        <activity android:name=".EditSchedule" />
        <activity android:name=".ExerciseDrillDown" />
        <activity android:name=".ListOfSaved" />
        <activity android:name=".CircuitDisplay" />
        <activity android:name=".Generate" />
        <activity android:name=".OptionsActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>